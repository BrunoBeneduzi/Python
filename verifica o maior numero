a = int(input("diigte o PRIMEIRO numero "))

b = int(input("digite o SEGUNDO numero "))

if a == b or b == a:
    print("os valores {} e {} são iguais".format(a,b))
    
elif a > b:
        print("o valor {} é maior que o valor {}".format(a,b))
elif a < b:
    print("o valor {} é maior que o valor {}".format(b,a))

